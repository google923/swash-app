{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "**/*.html", "use": "@vercel/static" },
    { "src": "**/*.css", "use": "@vercel/static" },
    { "src": "**/*.json", "use": "@vercel/static" },
    { "src": "**/*.js", "use": "@vercel/static" },
    { "src": "assets/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/$", "dest": "/home.html" },
    { "src": "/api/createInstantPayLink", "dest": "/api/createInstantPayLink.js" },
    { "src": "/api/checkPaymentStatus", "dest": "/api/checkPaymentStatus.js" },
    { "src": "/api/zohoInboundEmail", "dest": "/api/zohoInboundEmail.js" },
    { "src": "/api/confirmBooking", "dest": "/api/confirmBooking.js" },
    { "src": "/api/createSubscriberMandate", "dest": "/api/createSubscriberMandate.js" },
    { "src": "/api/finaliseSubscriberMandate", "dest": "/api/finaliseSubscriberMandate.js" },
    { "src": "/api/deleteSubscriber", "dest": "/api/deleteSubscriber.js" },
    { "src": "/api/gcWebhook", "dest": "/api/gcWebhook.js" },
    { "src": "/api/aiHelper", "dest": "/api/aiHelper.js" },
    { "src": "/confirm-booking", "dest": "/api/confirmBooking.js" },
    { "handle": "filesystem" }
  ]
}
